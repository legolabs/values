{
  "name": "legolabs/values",
  "type": "library",
  "description": "Alter ego objects. Highly efficient way of building models, api clients and so on",

  "keywords": ["model"],
  "license": "MIT",
  "authors": [
    {
      "name": "Max Kotliar",
      "email": "kotlyar.maksim@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": { "Legolabs\\Values\\": "src" },
    "files": ["src/functions_include.php"]
  },
  "autoload-dev": {
    "psr-4": { "Legolabs\\Values\\Tests\\": "tests" }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.5-dev"
    }
  }
}
